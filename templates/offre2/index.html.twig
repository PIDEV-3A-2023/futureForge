{% extends 'base.html.twig' %}

{% block title %}Offre2 index
{% endblock %}

{% block body %}
	<h1>Offre ABONNEMENT
	</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Nom</th>
				<th>Description</th>
				<th>Reduction</th>
				<th>Type</th>
				<th>Dated</th>
				<th>Datef</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for offre2 in offre2s %}
				<tr>
					<td>{{ offre2.nom }}</td>
					<td>{{ offre2.description }}</td>
					<td>{{ offre2.reduction }}</td>
					<td>{{ offre2.type }}</td>
					<td>{{ offre2.dated ? offre2.dated|date('Y-m-d') : '' }}</td>
					<td>{{ offre2.datef ? offre2.datef|date('Y-m-d') : '' }}</td>
					<td>
						<a href="{{ path('app_offre2_show', {'id': offre2.id}) }}">show</a>
						<a href="{{ path('app_offre2_edit', {'id': offre2.id}) }}">edit</a>

						{{ include('offre2/_delete_form.html.twig') }}
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="8">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_offre2_new') }}">Create new</a>
{% endblock %}
